@model IEnumerable<DigitalPepak.Kewan>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NamaKewan)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AnakKewan)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NamaKewan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AnakKewan)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.KewanId }) |
            @Html.ActionLink("Details", "Details", new { id=item.KewanId }) |
